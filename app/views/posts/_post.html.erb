<div id="<%= dom_id post %>" class="text-center post-container col-6" style="margin: auto;">
  <p>
    <%= post.titles %>
  </p>
  <p>
    <%= post.description %>
  </p>
  <p>
    <%= post.keywords %>
  </p>
  <p>
    <%= post.user.email %>
  </p>
  <p>
    <%= link_to "", post, class: "post-link" %>
  </p>
  <% post.images.each do |image|%>
    <%= image_tag(image, style: "
      max-width: 50%;
      height: auto; object-fit: contain;") %>
  <% end %>
</div>
<p>
  <%= link_to post.user.email, post.user , class: "z-index" %>
</p>